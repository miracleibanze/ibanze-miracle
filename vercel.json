{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)\.(js|css|png|jpg|jpeg|svg|ico|woff|woff2|ttf|eot|mp4|webm|json|pdf)$",
      "destination": "/$1.$2"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
